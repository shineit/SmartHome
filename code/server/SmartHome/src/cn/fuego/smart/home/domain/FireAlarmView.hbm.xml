<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.smart.home.domain">
	<class name="FireAlarmView" table="fire_alarm_view">

	    <id name="id" column="ID" >
	       <generator class="identity"/> 
		</id>
		 <property name="concentratorID" column="CONCENTRATOR_ID"></property>
		 <property name="objType" column="OBJ_TYPE"></property>
 		 <property name="objID" column="OBJ_ID"></property>
		 <property name="alarmType" column="ALARM_TYPE"></property>
		 <property name="alarmTime" column="ALARM_TIME"></property>
		 <property name="clearStatus" column="CLEAR_STATUS"></property>
         <property name="dataValue" column="DATA_VALUE"></property>
		 <property name="clearUser" column="CLEAR_USER"></property>
		 <property name="clearTime" column="CLEAR_TIME"></property>
		 
		 <property name="status" column="STATUS"></property>
		 <property name="concentDesp" column="CONCENT_DESP"></property>
		 <property name="planID" column="PLAN_ID"></property>
		 
		 <property name="machineID" column="MACHINE_ID"></property>
		 <property name="loopID" column="LOOP_ID"></property>
		 <property name="codeID" column="CODE_ID"></property>
		 <property name="locationDesp" column="LOCATION_DESP"></property>
		 <property name="locationX" column="LOCATION_X"></property>
		 <property name="locationY" column="LOCATION_Y"></property>
		 <property name="sensorType" column="SENSOR_TYPE"></property>
		 <property name="sensorTypeName" column="SENSOR_TYPE_NAME"></property>	
		 
		 <property name="alarmTypeName" column="ALARM_TYPE_NAME"></property>
		 <property name="kind" column="KIND"></property>
		 <property name="isPush" column="IS_PUSH"></property>
		 <property name="pushType" column="PUSH_TYPE"></property>
	</class>
 
</hibernate-mapping>